<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape >
            <solid android:color="@android:color/white" />
            <stroke
                android:dashGap="0dp"
                android:dashWidth="5dp"
                android:width="1dp"
                android:color="@android:color/black" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
</layer-list>